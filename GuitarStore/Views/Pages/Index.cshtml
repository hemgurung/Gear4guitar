@model GuitarStore.Models.ViewModels.Pages.PageVM

@{
    ViewBag.Title = ViewBag.PageTitle;
    TempData["PageTitle"] = ViewBag.PageTitle;
}

@if (TempData["SM"] != null)
{
    <div class="container row">
        <div class="col-md-12 text-center alert alert-dismissible alert-info">
            <button type="button" class="close" data-dismiss="alert">&times;</button>
            @TempData["SM"]
        </div>
    </div>
}

@Html.Raw(Model.Body)